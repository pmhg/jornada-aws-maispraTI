{
  "name": "mentoria-api-aws",
  "version": "1.0.0",
  "description": "",
  "main": "handler.js",
  "scripts": {
    "prerun:dev": "export NODE_ENV=dev && ./resources/dev/sqs/execute.sh && npm run build",
    "prerun:devwin": "export NODE_ENV=dev && npm run build",
    "deploy:dev": "export NODE_ENV=dev && export $(cat .env.dev | xargs) && npm run prerun:dev && export SLS_DEBUG=* && node_modules/serverless/bin/serverless offline start --noAuth",
    "deploy:devwin": "export NODE_ENV=dev && npm run prerun:devwin && export SLS_DEBUG=* && sls offline start --noAuth",
    "deploy:hlg": "export NODE_ENV=hlg && npm run deploy",
    "deploy:prd": "export NODE_ENV=prd && npm run deploy",
    "deploy:prdwin": "export NODE_ENV=prd && export ENV=prd && npm run deploy",
    "package:dev": "export NODE_ENV=dev && npm run package",
    "package:hlg": "export NODE_ENV=hlg && npm run package",
    "package:prd": "export NODE_ENV=prd && npm run package",
    "debug:dev": "export NODE_ENV=dev && export $(cat .env.dev | xargs) && npm run prerun:dev && export SLS_DEBUG=* && node_modules/serverless/bin/serverless offline start --noAuth",
    "test": "jest --no-cache --silent",
    "build": "tsc",
    "deploy": "npm run build && serverless deploy",
    "package": "npm run build && serverless package",
    "tslint": "tslint -c tslint.json 'src/**/*.ts'",
    "npm-audit-html": "npm-audit-html"
  },
